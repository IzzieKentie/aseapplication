                        <input type="image" alt="In Progress" class="icons" src="in-progress.png">
                      </form>
                    </div>
                    </div>
                  <% } } %>
                    </div>
                </div>
          <% } %>
      </div>
    </div>
     <div class="menu_and_event">
      <div class="flex-event-selected-fotd">
        <div class="flex-event-selected-item">
          <h3>Flow of The Day</h3>
        </div>
        <div class="flex-event-selected-item" >
          <button type="button" class="btn btn-info select-event" data-toggle="collapse" data-target="#newModule" style="width:100%"><p class="task_text">New Module</p></button><br>
        </div>
        <div class="flex-event-selected-item" >
          <div id="newModule" class="collapse" style="width:100%">
            <div class="newModule">
              <form action="/newModule" method="POST">
                <p>Module Name</p>
                <input type="text" name="module_name" style="padding: 0 0 0 0; width: 50%">
                <p>Module Leader:</p>
                <input type="text" name="module_leader" style="padding: 0 0 0 0; width: 50%">
                <p>Module Location: </p>
                <input type="text" name="module_location" style="padding: 0 0 0 0; width: 50%">
                <p>Module Start Time: </p>
                <input type="time" min="08:00" max="19:00" name="module_start_time" style="padding: 0 0 0 0; width: 50%">
                <p>Module End Time: </p>
                <input type="time" min="08:00" max="19:00" name="module_end_time" style="padding: 0 0 0 0; width: 50%">
                <input type = "hidden" name = "event_id" value = "<%=event[0].event_id%>" style="padding: 0 0 0 0; width: 50%"/>
                <input type="submit" class="select-event" style="padding: 0 0 0 0; width: 50%"> 
              </form>
            </div>
          </div>
        </div>
        <% if(modules.length){ 
          for(var k = 0;k < modules.length;k++) { %>
          <form class="icon" method="POST" action="/saveModule" style="width:100%; display: flex">
            <div class="flex-event-selected-item module">
              <div class="flex-event-selected-item">
                <img id="<%=modules[k].module_id%>_edit_icon" class="icons" style="float:left; height: 20px" src="edit.png">
                <input type="image" id="<%=modules[k].module_id%>_save_icon" alt="Save" class="icons" style="float:left; height: 20px" src="save.png" hidden>
              </div>
              <script>
                document.getElementById("<%=modules[k].module_id%>_edit_icon").onclick = function () {
                  document.getElementById("<%=modules[k].module_id%>_name_text").hidden = true;
                  document.getElementById("<%=modules[k].module_id%>_location_text").hidden = true;
                  document.getElementById("<%=modules[k].module_id%>_leader_text").hidden = true;
                  document.getElementById("<%=modules[k].module_id%>_start_text").hidden = true;
                  document.getElementById("<%=modules[k].module_id%>_end_text").hidden = true;
                  document.getElementById("<%=modules[k].module_id%>_name_edit").type = "text";
                  document.getElementById("<%=modules[k].module_id%>_location_edit").type = "text";
                  document.getElementById("<%=modules[k].module_id%>_leader_edit").type = "text";
                  document.getElementById("<%=modules[k].module_id%>_start_edit").type = "text";
                  document.getElementById("<%=modules[k].module_id%>_end_edit").type = "text";
                  document.getElementById("<%=modules[k].module_id%>_edit_icon").hidden = true;
                  document.getElementById("<%=modules[k].module_id%>_save_icon").hidden = false;
                };
              </script>
              <input type="hidden" name="module_id" value="<%=modules[k].module_id%>">
              <div class="flex-event-selected-item">
                <h4 id="<%=modules[k].module_id%>_name_text"><%=modules[k].module_name%></h4><br><br>
                <input style="padding: 0 0 0 0;  height: 30px;  width: 95%;" type="hidden" name="module_name" id="<%=modules[k].module_id%>_name_edit" value="<%=modules[k].module_name%>">
              </div>
              <div class="flex-event-selected-item">
                <p id="<%=modules[k].module_id%>_location_text" class="moduleText"><b>Location: </b><%=modules[k].module_location%></p>
                <input style="padding: 0 0 0 0;  height: 30px;  width: 95%;" type="hidden" name="module_location" id="<%=modules[k].module_id%>_location_edit" value="<%=modules[k].module_location%>">
                </div>
              <div class="flex-event-selected-item">
                <p id="<%=modules[k].module_id%>_leader_text" class="moduleText"><b>Leader: </b><%=modules[k].module_leader%></p>
                <input style="padding: 0 0 0 0;  height: 30px;  width: 95%;" type="hidden" name="module_leader" id="<%=modules[k].module_id%>_leader_edit" value="<%=modules[k].module_leader%>">
                </div>
              <div class="flex-event-selected-item">
                <p id="<%=modules[k].module_id%>_start_text" class="moduleText"><b>Start Time: </b><%=modules[k].module_start_time%></p>
                <input style="padding: 0 0 0 0;  height: 30px;  width: 95%;" type="hidden" name="module_start" id="<%=modules[k].module_id%>_start_edit" value="<%=modules[k].module_start_time%>">
                </div>
              <div class="flex-event-selected-item">
                <p id="<%=modules[k].module_id%>_end_text" class="moduleText"><b>End Time: </b><%=modules[k].module_end_time%></p>
                <input style="padding: 0 0 0 0;  height: 30px;  width: 95%;" type="hidden" name="module_end" id="<%=modules[k].module_id%>_end_edit" value="<%=modules[k].module_end_time%>">
              </div>
            </div>
          </form>
        <% } } %>
      </div>
    <div class="flex-event-selected-notes">
        <div class="flex-event-selected-item">
          <h3>Notes/Other</h3>
        </div>
        <% if(info.length){ %>
        <div class="flex-event-selected-item">
          <form method="POST" action="/saveNotes" id="add_notes" style="width: 90%">
            <input type="hidden" name="request_type" value="UPDATE"></input>
            <input type="hidden" name="info_id" value="<%=info[0].info_id%>"></input>
            <input type="hidden" name="event_id" value="<%=info[0].event_id%>"></input>
            <input type="hidden" name="member_id" value="<%=info[0].member_id%>"></input>
            <textarea name="event_notes" form="add_notes" value="<%=info[0].event_notes%>" class="notes"><%=info[0].event_notes%></textarea>
            <br><br>
            <input type="submit" class="select-event" value="Save"></input>
          </form>
        </div>
          <% 
     }else{ %>
        <div class="flex-event-selected-item">
          <form method="POST" action="/saveNotes" id="add_notes" style="width: 90%">
            <input type="hidden" name="request_type" value="INSERT"></input>
            <input type="hidden" name="event_id" value="<%=event[0].event_id%>"></input>
            <textarea name="event_notes" form="add_notes" class="notes"></textarea>
            <br><br>
            <input type="submit" class="select-event" value="Save"></input>
          </form>
        </div>
      <% } %>
      </div>
    </div>
  </div>
      </div>
</div>
 <div style="display: none">Icons made by <a href="https://www.flaticon.com/authors/smashicons" title="Smashicons">Smashicons</a> from <a href="https://www.flaticon.com/" title="Flaticon">www.flaticon.com</a></div>
</body>
